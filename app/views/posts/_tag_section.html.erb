<div class="tag__section--search">
  <div class="tag-title">検索</div>
    <%= form_for(post, url: {controller: 'posts', action: 'search' }, html: {method: 'GET'}) do |f| %>
      <br>
      <div class="form_check-line form_check-sub form-check-inline check_box">
        <% tags.each do |tag| %>
          <div class="check-group">
            <%= f.check_box :tag_list , { multiple: true , class: 'interested input' }, "#{tag.name}", nil %>
            <%= f.label " #{tag.name}(#{tag.taggings_count})", class: 'interested label tag-btn'%>
          </div>
        <% end %>
      </div>
      <%= f.submit "検索", class: 'submit btn btn-outline-primary' %>
    <% end %>
  </div>
</div>